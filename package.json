    "@types/bcrypt": "^5.0.0",
    "apollo-server-express": "^3.13.0",
    "aws-sdk": "^2.1528.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.11.0",
    "http-status": "^1.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
